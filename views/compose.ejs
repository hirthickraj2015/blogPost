<h1>Compose</h1>
<form method="post" action="/compose">
    <div class="form-group">
        <label for="title">Title</label><br>
        <input type="text" name="title" class="form-control"><br>
        <label for="data">Post</label><br>
        <textarea name="data" rows="4" cols="102" class="form-control"></textarea><br>
        <button type="submit" class="btn btn-primary" aria-placeholder="Publish">Publish</button>
    </div>    
</form>

